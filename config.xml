<?xml version='1.0' encoding='utf-8'?>
<widget id="io.star.student" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Star Student</name>
    <description>
        Star student is scheduler for students school activities for productivity
    </description>
    <author email="goodwish@axxess.co.za" href="http://www.orenlabs.com">
       goodwish@axxess.co.za
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" /> 
    </platform>
    <icon src="res/icon.png" />
    <preference name="KeepRunning" value="true"/>
     <preference name="android-minSdkVersion" value="26"/> 
     <preference name="android-targetSdkVersion" value="27"/>
</widget>
